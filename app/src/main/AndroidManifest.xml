<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.startandroid.scopeproject">

    <application
        android:name="ru.example.scopeproject.App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="ru.example.scopeproject.login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="ru.example.scopeproject.folders.FolderListActivity"
            android:label="@string/folders"/>
        <activity android:name="ru.example.scopeproject.letters.LetterListActivity"
            android:label="@string/letters"/>
    </application>

</manifest>